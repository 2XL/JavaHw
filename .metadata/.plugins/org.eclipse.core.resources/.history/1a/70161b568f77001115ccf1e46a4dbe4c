package TADs;

public class NodeLinker<Code, Elem> {
	
	private Code codigo;
	private Elem elemento;
	private int sig;
	private int ant;
	
	public NodeLinker(Code cod, Elem ele, int next, int last){
		codigo=cod;
		elemento=ele;
		sig=next;
		ant=last;	
	}
	
	
	
	
	
	
	
	
	//Node<Code, Element>
	
	// implementacio estatica o dinamica
	// estatica ==  seran dos enters
	// dinamica ==  dos punters
	
// 
	
	
	
	
	
}
