		int dimServer=0, typeServer=0;				// especificar el tipus de serividor que farem servir i la capactiat maxima que tindra aquest servidor

		
		
		
		
		typeServer = Integer.parseInt(teclat.readLine());	
		
		
		int aux;
		
		String hk;
		
		do{
			System.out.println("implementando HASHCODE");
			
			System.out.println("indique el correo ");
			
			
			hk = teclat.readLine();
			
			
			System.out.println(hk+" -> "+hk.hashCode());
			
			
			
			
			
			
			
			
			
			
			
System.out.println("salir = 0");			
			
			
			
			
			
		 aux = Integer.parseInt(teclat.readLine());
			

			
		}while(aux !=0 );
